{
  "compilerOptions": {
    "target": "es2017",
    "lib": [
      "es2017"
    ],
    "module": "commonjs",
    "rootDir": "src",
    "outDir": "dist",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "src",
    
  ], 
  "exclude": [
    "node_modules",
    "dist"
  ]
}